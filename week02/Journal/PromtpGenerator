using System;
using System.Collections.Generic;

public class PromptGenerator
{
    public List<string> _prompts = new List<string>
    {
        "Quem foi a pessoa mais interessante que você conheceu hoje?",
        "Qual foi a coisa mais difícil que você teve que fazer hoje?",
        "Qual foi a maior surpresa que você teve hoje?",
        "O que te fez sorrir hoje?",
        "Se você pudesse reviver um momento de hoje, qual seria?"
    };

    public string GetRandomPrompt()
    {
        Random random = new Random();
        int index = random.Next(0, _prompts.Count);
        return _prompts[index];
    }
}