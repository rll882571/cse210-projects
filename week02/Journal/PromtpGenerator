using System;
using System.Collections.Generic;

public class PromptGenerator
{
    public List<string> _prompts = new List<string>
    {
        "Who was the most interesting person you met today?",
        "What was the most difficult thing you had to do today?",
        "What was the biggest surprise you had today?",
        "What made you smile today?",
        "If you could relive one moment from today, what would it be?"
    };

    public string GetRandomPrompt()
    {
        Random random = new Random();
        int index = random.Next(0, _prompts.Count);
        return _prompts[index];
    }
}
